#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    
    vector<int> ans;
    int n,a;
    cin>>n;
    while(n!=0){
       a=n%10;
        ans.push_back(a);
        n=n/10;
    }
    sort(ans.begin(),ans.end());
  
    cout<<ans[0]+ans[ans.size()-1];
    

    return 0;
}
